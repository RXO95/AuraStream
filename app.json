        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rxo95.AuraStream"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f1b0332c-ffa7-4e8a-a9af-67b21e178376"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
